          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <transformers>
                <transformer>
                  <mainClass>F:\workspace\api_auto_7_v9.2_shizhan\src\test\java\com\qj\api\auto\cases\Inport.java</mainClass>
